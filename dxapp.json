{
  "name": "dx-ssh-demo",
  "title": "DNAnexus SSH Demo",
  "summary": "dx-ssh-demo",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "categories": [],
  "inputSpec": [],
  "outputSpec": [],
  "runSpec": {
    "interpreter": "bash",
    "file": "src/dx-ssh-demo.sh",
    "execDepends": [
      {"name": "htop"},
      {"name": "byobu"},
      {"name": "dxssh",
       "package_manager": "git",
       "url": "https://github.com/kislyuk/dxssh.git",
       "build_commands": "cp -R * /; cd /etc/update-motd.d; rm -f *; ln -s /usr/bin/dx-motd; cd /usr/bin; rm dx-exec-helper; mv /u2/bin/dx-exec-helper ."
      }
    ]
  },
  "access": {"network": ["*"]},
  "authorizedUsers": []
}
